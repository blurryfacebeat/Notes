const a = 14.555;
// Проверяет, конечное ли число 
isFinite(a);
// Переводит в 16-ричную система счисления
a.toString(16);
// Переводит в 2-ичную система счисления
a.toString(2);
// Проверяет не число ли это
isNaN(a);
// Округляем до 2-х знаков после запятой, toFixed() возращает строку, поэтому можно написать +a.toFixed(2);
a.toFixed(2);
// Округляем
Math.round(a);
// Отбрасываем дробную часть
Math.floor(a);
// Ищет ближайшее целое число, которое больше и округляет до него
Math.ceil(a);

// Вернет целое число, в доп. параметре указываем систему счисления
parseInt('100px', 10);
// Вытаскивает уже десятичное число
parseFloat('25.5 рублей');
